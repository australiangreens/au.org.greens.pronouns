<?xml version="1.0"?>
<extension key="au.org.greens.pronouns" type="module">
  <file>pronouns</file>
  <name>Pronouns Extension</name>
  <description>Add in ability to record someone's pronoun via event or contribution pages and profiles</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus@nsw.greens.org.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/au.org.greens.pronouns</url>
    <url desc="Documentation">https://github.com/australiangreens/au.org.greens.pronouns</url>
    <url desc="Support">https://github.com/australiangreens/au.org.greens.pronouns</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-10-02</releaseDate>
  <version>1.3.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Pronouns</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Pronouns_Upgrader</upgrader>
  <mixins>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
